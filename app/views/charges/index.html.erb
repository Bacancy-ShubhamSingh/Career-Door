        <h2 class="title-1 m-b-25">Companies list</h2>
        <div class="au-card au-card--bg-blue au-card-top-countries m-b-40">
          <div class="au-card-inner">
            <div class="table-responsive">
              <table class="table table-hover">
                <thead class="thead-light">
                  <tr>
                    <th>Company Name</th>
                    <th>Subscription Taken?</th>
                    <th>Trial days</th>
                    <th>Subscription Date</th>
                  </tr>
                </thead>
                <tbody id="companies_subscription_list_body">
                  <%= render "companies_list.html.erb" %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="main-content">
  <div class="section__content section__content--p30">
    <div class="container-fluid">
      <div class="col-lg-12">
        <div class="au-card au-card--bg-blue au-card-top-countries m-b-40">
          <div class="au-card-inner">
            <div class="table-responsive">
              <table class='table'>
                <thead>
                  <tr>
                    <th>Technology Name</th>
                  </tr>
                </thead>
                <tbody>
                  <% @all_technologies.each do |technology| %>
                    <tr>
                      <td><%= technology.technology_name %></td>
                      <td><%= link_to "edit", edit_technology_path(technology.id) %></td>
                      <td><%= link_to "show", technology_path(technology.id) %></td>
                      <td>
                        <%= link_to "delete", technology_path(technology.id), :confirm => "are you sure?", data: { method: :delete } %>
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

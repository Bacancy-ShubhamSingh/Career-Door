<% # <% binding.pry %>
<% if !@jobs.blank? %>
  <% @jobs.each do |job| %>
    <tr>
      <td title ="Job Role"><%= job.job_role %></td>
      <td title ="Technology"><%= job.technology.technology_name %></td>
      <td title ="Experience"><%= job.experience %></td>
      <td title ="CGPA"><%= job.cgpa %></td>
      <td><%= link_to "Open", user_opening_job_path(current_user, job) %></td>
    </tr>
  <% end %>
<% else %>
  <span style="color: red; font-weight: bold">***Data not available</span>
<% end %>
